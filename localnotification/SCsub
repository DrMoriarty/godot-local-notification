
Import('env')

if env['platform'] == 'iphone':
    env.add_source_files(env.modules_sources, "ios/*.mm")
    env.add_source_files(env.modules_sources, "ios/*.m")
    env.add_source_files(env.modules_sources, "*.cpp")

elif env['platform'] == 'android':
    env.add_source_files(env.modules_sources, "*.cpp")

